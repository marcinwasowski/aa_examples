        <activity
            android:name="pl.amsard.aa3examples.std.ExampleFragmentActivity"
            android:label="@string/title_activity_example_fragment" >
        </activity>
        <activity
            android:name="pl.amsard.aa3examples.aa.AAExampleFragmentActivity_"
            android:label="@string/title_activity_example_fragment" >
        </activity>
        
        <activity
            android:name="pl.amsard.aa3examples.std.ThreadActivity"
            android:label="@string/title_activity_example_fragment" >
        </activity>
        <activity
            android:name="pl.amsard.aa3examples.aa.AAThreadActivity_"
